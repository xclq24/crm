package com.tarena.crm.entity;

import java.util.Date;

public class Record {
	private int id;
	private String name;
	private Date time;
	private String type;
	private Date next;
	private String contacter;
	private String theme;
	private String note;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Date getTime() {
		return time;
	}
	public void setTime(Date time) {
		this.time = time;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public Date getNext() {
		return next;
	}
	public void setNext(Date next) {
		this.next = next;
	}
	public String getContacter() {
		return contacter;
	}
	public void setContacter(String contacter) {
		this.contacter = contacter;
	}
	public String getTheme() {
		return theme;
	}
	public void setTheme(String theme) {
		this.theme = theme;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	
	public Object[] toArray() {
		return new Object[] {
				name,time,type,next,contacter,theme,note
		};
	}
	@Override
	public String toString() {
		return "Record [id=" + id + ", name=" + name + ", time=" + time
				+ ", type=" + type + ", next=" + next + ", contacter="
				+ contacter + ", theme=" + theme + ", note=" + note + "]";
	}
	
}
